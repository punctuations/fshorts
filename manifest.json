{
  "manifest_version": 3,
  "name": "fshorts",
  "version": "1.0.0",
  "description": "F*ck shorts.",
  "permissions": [],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/@*",
        "https://www.youtube.com/c/*",
        "https://www.youtube.com/channel/*",
        "https://www.youtube.com/user/*"
      ],
      "js": ["channel/home.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://www.youtube.com/results*"],
      "js": ["results.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["watch.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
